            "Blood Price": [
                "var condition = source.HasBuff(\"AatroxWPower\") && source.HasBuff(\"AatroxWONHPowerBuff\");",
                "return condition ? PassiveBase[source.Spellbook.GetSpell(SpellSlot.W).Level - 1] + PassiveBonusAd * source.FlatPhysicalDamageMod : 0d;"
            ]
        },
        "Q": {
            "Stages": 1,
            "Stage0": [
                "return Damage.CalculateDamage(source, target, DamageType.Physical, QBase[source.Spellbook.GetSpell(SpellSlot.Q).Level - 1] + QBonusAd * source.FlatPhysicalDamageMod);"
            ]
        },
        "E": {
            "Stages": 1,
            "Stage0": [
                "return Damage.CalculateDamage(source, target, DamageType.Magical, EBase[source.Spellbook.GetSpell(SpellSlot.Q).Level - 1] + EAP * source.TotalMagicalDamage + EBonusAD * source.FlatPhysicalDamageMod);"
            ]
        },
        "R": {
            "Stages": 1,
            "Stage0": [
                "return Damage.CalculateDamage(source, target, DamageType.Magical, RBase[source.Spellbook.GetSpell(SpellSlot.Q).Level - 1] + RAP * source.TotalMagicalDamage);"
            ]
        }
    },
